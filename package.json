{
  "name": "minidocs",
  "version": "1.1.1",
  "description": "build a simple site for your documentation",
  "main": "index.js",
  "style": "components/styles/index.css",
  "scripts": {
    "lint": "standard",
    "build:example": "browserify example/index.js -o example/bundle.js",
    "deploy:example": "surge example minidocs-example.surge.sh",
    "build": "browserify docs/index.js -o docs/bundle.js",
    "deploy": "npm run build && surge site minidocs.surge.sh"
  },
  "bin": {
    "minidocs": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freeman-lab/minidocs.git"
  },
  "keywords": [
    "docs",
    "documentation",
    "site",
    "build",
    "component"
  ],
  "author": "freeman-lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freeman-lab/minidocs/issues"
  },
  "browserify": {
    "transform": [
      "brfs",
      "folderify"
    ]
  },
  "homepage": "https://github.com/freeman-lab/minidocs#readme",
  "dependencies": {
    "brfs": "^1.4.3",
    "browserify": "^13.0.0",
    "camelcase": "^2.1.1",
    "dom-css": "^2.0.0",
    "folderify": "^1.2.0",
    "from2-string": "^1.1.0",
    "highlight.js": "^9.2.0",
    "include-folder": "^1.0.0",
    "insert-css": "^0.2.0",
    "lodash.foreach": "^4.1.0",
    "lodash.isobject": "^3.0.2",
    "lodash.map": "^4.2.1",
    "marked": "^0.3.5",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "os-tmpdir": "^1.0.1",
    "rimraf": "^2.5.2",
    "simple-html-index": "^1.2.0"
  },
  "devDependencies": {
    "budo": "^8.1.0",
    "debug": "^2.2.0",
    "standard": "^6.0.8"
  }
}
